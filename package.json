{
  "name": "movie-database-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "set DEBUG=movie-app/* && nodemon --exec babel-node src/index.js",
    "lint": "eslint --ignore-path .gitignore src/ --fix",
    "prettier": "prettier --single-quote --trailing-comma es5 --print-width 85 --write --no-bracket-spacing src/**/*.js",
    "build": "babel src --out-dir build --copy-files",
    "prebuild": "npm run lint && npm run clean && npm run prettier",
    "clean": "rm -rf build/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniellamarr/movie-database-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daniellamarr/movie-database-app/issues"
  },
  "homepage": "https://github.com/daniellamarr/movie-database-app#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "engines": {
    "node": "12",
    "npm": "6.13.0"
  }
}
